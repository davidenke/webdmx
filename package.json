{
  "name": "@webdmx/monorepo",
  "version": "0.0.1",
  "description": "A DMX interface for a webview using WebUSB",
  "keywords": [
    "webdmx",
    "ui",
    "usb"
  ],
  "author": {
    "name": "David Enke",
    "email": "david@enke.dev"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "check": "pnpm -r --parallel --filter '@webdmx/*' check",
    "lint": "pnpm -r --parallel --filter '@webdmx/*' lint",
    "test": "pnpm -r --parallel --filter '@webdmx/*' --if-present test",
    "dev": "pnpm -r --parallel --filter-prod @webdmx/ui... dev",
    "build": "pnpm -r --filter @webdmx/ui... build"
  }
}
